{
  "name": "todo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "server": "http-server -p 1234",

    "watch:js": "watchify -t [ babelify --presets [ es2015 react ] ] src/app/app.js -o src/bundle.js -v",
    "watch:css": "node-sass styles/scss/styles.scss styles/css/styles.css --output-style compressed --source-map css/ -w",

    "build:js": "browserify -t [ babelify --presets [ es2015 react ] ] src/app/app.js -o src/bundle.js",
    "build:css": "node-sass styles/scss/styles.scss styles/css/styles.css --output-style compressed --source-map css/",

    "build": "npm run build:js && npm run build:css",
    "watch": "npm run watch:js && npm run watch:css",
    "postinstall": "npm run build"
  },
  "author": "",
  "dependencies": {
    "material-ui": "^0.15.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "node-sass": "^3.10.0",
    "watchify": "^3.7.0"
  },
  "license": "ISC"
}
